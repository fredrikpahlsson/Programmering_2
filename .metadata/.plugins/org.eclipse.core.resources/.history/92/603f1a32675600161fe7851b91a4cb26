/**Mammalklassen ärver från Animalklassen*/
public abstract class Mammal extends Animal {
	private int GTime;

	//Constructor
    /**skapar en Mammal med en String-variabel latinName, int-variablel och gestationTime*/
	public Mammal(String latinName, int GTime) {
		super(latinName);
		this.GTime = GTime;
	}

	//Methods
	/**Sätter GTime av Mammal*/
	public int getGTime() {
		return this.GTime;
	}

}