public class Cat extends Mammal {

	private int Levels;

	//Constructors
	/** skapar en stringvariabel för en cats latinska namn samt gravititet samt antal liv den har sammanlagt*/
	public Cat(String latinName, int GTime, int Levels) {
		super(latinName, GTime);
		this.Levels = Levels;
	}
	
	//Methods
	/**Inherits and implements the abstract method getInfo from class Animal*/
	public String getInfo() {
		return "The cat named 'noname' lat: '" + latinName + "' nurses for " + super.getGTime() + " months and has " + Levels + " lives";
	}
	/**Gets numberOfLives for the Cat*/
	public int getLevels() {
		return this.Levels;
	}
	/**Sets numberOfLives for the Cat*/
	public void setLevels(int i) {
		this.Levels = i;
	}	
}