public class Bike {
	
private String color; 
private int price;
private int size;


public Bike(String color, int size){
this.color = color; 
this.size = size;
}


public Bike(String color, int size, int price) {
this.size = size;
this.price = price;
}


public String getColor() {
	return this.color;
}


public int getPrice() {
	return this.price;
}
public int getSize() {
	return this.size;
}
}

