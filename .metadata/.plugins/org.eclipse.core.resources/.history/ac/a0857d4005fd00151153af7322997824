public abstract class Animal {
	
	protected String latinName;
    protected String Name;


	/**Skapar en Stringvariabel för de latisnska namnet för ett djur*/
	public Animal(String latinName) {
		this.latinName = latinName;
		
	}

	/**Denna metoden gör så att underklasserna ärver infon från animalclassen*/
	public abstract String getInfo();

	/**Sets the friendlyName of the Animal*/
	public void setFriendlyName(String name) {
		this.Name = name;
	}
	
	/**Sätter det döpta namnet på ett djur*/
	public String getName() {
		return this.Name;
	}
}